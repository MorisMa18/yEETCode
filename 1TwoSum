class Solution {
    public int[] twoSum(int[] nums, int target) {
        int diff = 0;
        int indicies[] = new int[2];
        for(int i = 0; i < nums.length-1; i++) {
            diff = target - nums[i];
            for(int j = i+1; j < nums.length; j++) {
                if(diff == nums[j]) {
                    indicies[0] = i;
                    indicies[1] = j;
                    return indicies;
                }
            }
        }
        return indicies;
    }
}